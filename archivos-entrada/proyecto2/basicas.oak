System.out.println("********** Operaciones Relacionales **********");

System.out.println("1. Igualdad");

System.out.println("1.1 int <-> int");

System.out.println(10 == 5); // false

System.out.println("1.2 float <-> float");
System.out.println(10.5 == 10.5); // true
System.out.println(10.5 == 5.5); // false

System.out.println("1.3 int <-> float");
System.out.println(10 == 10.5); // false
System.out.println(10 == 5.5); // false

System.out.println("1.4 var[int] <-> var[float]");
System.out.println(numero5 == numero6); // false

System.out.println("1.5 string <-> string");
System.out.println("Hola" == "Hola"); // true
System.out.println("Hola" == "Mundo"); // false
System.out.println("HolaMundo" == "Hola" + "Mundo"); // true

System.out.println("1.6 var[string] <-> string");
System.out.println(cadena2 == "Hola mundo"); // true

System.out.println("1.7 boolean <-> boolean");
System.out.println(true == true); // true
System.out.println(true == false); // false

System.out.println("1.8 char <-> char");
System.out.println('a' == 'a'); // true
System.out.println('a' == 'b'); // false

System.out.println("");

System.out.println("2. Diferencia");

System.out.println("2.1 int <-> int");
System.out.println(10 != 10); // false
System.out.println(10 != 5); // true

System.out.println("2.2 float <-> float");
System.out.println(10.5 != 10.5); // false
System.out.println(10.5 != 5.5); // true

System.out.println("2.3 int <-> float");
System.out.println(10 != 10.5); // true
System.out.println(10 != 5.5); // true

System.out.println("2.4 string <-> string");
System.out.println("Hola" != "Hola"); // false
System.out.println("Hola" != "Mundo"); // true

System.out.println("2.5 var[string] <-> string");
System.out.println(cadena2 != "Hola mundo"); // false

System.out.println("2.6 boolean <-> boolean");
System.out.println(true != true); // false
System.out.println(true != false); // true

System.out.println("2.7 char <-> char");
System.out.println('a' != 'a'); // false
System.out.println('a' != 'b'); // true

System.out.println("");

System.out.println("3. Mayor que");

System.out.println("3.1 int <-> int");
System.out.println(10 > 5); // true
System.out.println(10 > 10); // false

System.out.println("3.2 float <-> float");
System.out.println(10.5 > 5.5); // true
System.out.println(10.5 > 10.5); // false

System.out.println("3.3 int <-> float");
System.out.println(10 > 5.5); // true
System.out.println(10 > 10.5); // false
System.out.println(3.4 > 3); // true
System.out.println(3.4 > 3.5); // false

System.out.println("3.4 char <-> char");
System.out.println('a' > 'b'); // false
System.out.println('b' > 'a'); // true
System.out.println('a' > 'a'); // false

System.out.println("");

System.out.println("4. Menor que");

System.out.println("4.1 int <-> int");
System.out.println(10 < 5); // false
System.out.println(10 < 10); // false

System.out.println("4.2 float <-> float");
System.out.println(10.5 < 5.5); // false
System.out.println(10.5 < 10.5); // false

System.out.println("4.3 int <-> float");
System.out.println(10 < 5.5); // false
System.out.println(10 < 10.5); // true

System.out.println("4.4 char <-> char");
System.out.println('a' < 'b'); // true
System.out.println('b' < 'a'); // false
System.out.println('a' < 'a'); // false

System.out.println("");

System.out.println("5. Mayor o igual que");

System.out.println("5.1 int <-> int");
System.out.println(10 >= 5); // true
System.out.println(10 >= 10); // true

System.out.println("5.2 float <-> float");
System.out.println(10.5 >= 5.5); // true
System.out.println(10.5 >= 10.5); // true

System.out.println("5.3 int <-> float");
System.out.println(10 >= 5.5); // true
System.out.println(10 >= 10.5); // false

System.out.println("5.4 char <-> char");
System.out.println('a' >= 'b'); // false
System.out.println('b' >= 'a'); // true
System.out.println('a' >= 'a'); // true

System.out.println("");

System.out.println("6. Menor o igual que");

System.out.println("6.1 int <-> int");
System.out.println(10 <= 5); // false
System.out.println(10 <= 10); // true

System.out.println("6.2 float <-> float");
System.out.println(10.5 <= 5.5); // false
System.out.println(10.5 <= 10.5); // true

System.out.println("6.3 int <-> float");
System.out.println(10 <= 5.5); // false
System.out.println(10 <= 10.5); // true

System.out.println("6.4 char <-> char");
System.out.println('a' <= 'b'); // true
