System.out.println("********** Operaciones Logicas **********");

System.out.println("1. AND");
System.out.println(true && true); // true
System.out.println(true && false); // false
System.out.println(false && true); // false
System.out.println(false && false); // false
System.out.println(10 == 10 && 5 == 5); // true
System.out.println(10 == 10 && 5 == 6); // false
System.out.println("");

System.out.println("2. OR");
System.out.println(true || true); // true
System.out.println(true || false); // true
System.out.println(false || true); // true
System.out.println(false || false); // false
System.out.println(10 == 10 || 5 == 5); // true
System.out.println(10 == 10 || 5 == 6); // true
System.out.println(10 == 11 || 5 == 6); // false
System.out.println("");

System.out.println("3. NOT");
System.out.println(!true); // false
System.out.println(!false); // true
System.out.println(!(10 == 10)); // false
System.out.println(!(10 == 11)); // true
System.out.println("");

System.out.println("********** Operador Ternario **********");
System.out.println(true ? "verdadero" : "falso"); // verdadero
System.out.println(false ? "verdadero" : "falso"); // falso
System.out.println(10 == 10 ? "verdadero" : "falso"); // verdadero
System.out.println(10 == 11 ? "verdadero" : "falso"); // falso
System.out.println("");

